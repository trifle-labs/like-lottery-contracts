{
  "name": "@trifle/like-lottery-contracts",
  "version": "1.0.3",
  "description": "Smart contracts for Like Lottery",
  "main": "src/index.js",
  "types": "src/index.ts",
  "files": [
    "src",
    "contracts",
    "artifacts",
    "deployments",
    "typechain-types"
  ],
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./src/index.ts",
